   };\r\n  },\r\n  created() {\r\n    this.id = this.$route.params.id;\r\n    this.getEmpresa();\r\n  },\r\n  methods: {\r\n    editEmpresa() {\r\n      let taskData = {\r\n        id: this.task.id,\r\n        nomeempresa: this.task.nomeempresa,\r\n        telefone: this.task.telefone,\r\n        status: this.task.status,\r\n      };\r\n      axios\r\n        .put(\"http://localhost:3000/tasks/\" + this.id, taskData)\r\n        router.push({ name: \"home\" });\r\n    },\r\n    getEmpresa() {\r\n      axios\r\n        //get(`${server.baseURL}/customer/customer/${this.id}`)\r\n        .get(\"http://localhost:3000/tasks\")\r\n        .then((data) => (this.task = data.data));\r\n    },\r\n    //Voltar para Home\r\n    navigate() {\r\n      router.go(-1);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components/customer"}]}