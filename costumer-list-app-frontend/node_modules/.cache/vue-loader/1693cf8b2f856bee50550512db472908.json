el for=\"title\"> Nome da Empresa </label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"nomeempresa\"\r\n            v-model=\"task.nomeempresa\"\r\n            name=\"title\"\r\n            class=\"form-control\"\r\n            placeholder=\"Enter Nome Empresa\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group col-md-12\">\r\n          <label for=\"title\"> Telefone </label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"telefone\"\r\n            v-model=\"task.telefone\"\r\n            name=\"title\"\r\n            class=\"form-control\"\r\n            placeholder=\"Enter Telefone\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group col-md-12\">\r\n          <label for=\"title\"> Status </label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"status\"\r\n            v-model=\"task.status\"\r\n            name=\"title\"\r\n            class=\"form-control\"\r\n            placeholder=\"Enter Status\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group col-md-4 pull-right\">\r\n          <button class=\"btn btn-success\" type=\"submit\">Editar Empresa</button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { server } from \"../../helper\";\r\nimport axios from \"axios\";\r\nimport router from \"../../router\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: 0,\r\n      customer: {},\r\n    };\r\n  },\r\n  created() {\r\n    this.id = this.$route.params.id;\r\n    this.getEmpresa();\r\n  },\r\n  methods: {\r\n    editEmpresa() {\r\n      let customerData = {\r\n        nomeempresa: this.customer.nomeempresa,\r\n        telefone: this.customer.telefone,\r\n        status: this.customer.status,\r\n      };\r\n      axios\r\n        .put(\r\n          `${server.baseURL}/customer/update?taskID=${this.id}`,\r\n          customerData\r\n        )\r\n        .then((data) => {\r\n          router.push({ name: \"home\" });\r\n        });\r\n    },\r\n    getEmpresa() {\r\n      axios\r\n        .get(`${server.baseURL}/customer/customer/${this.id}`)\r\n        .then((data) => (this.customer = data.data));\r\n    },\r\n    navigate() {\r\n      router.go(-1);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}